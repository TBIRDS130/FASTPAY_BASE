{
  "name": "fastpay",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,yml,yaml}\"",
    "preview": "vite preview",
    "start:production": "node server.production.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "create-user": "node scripts/create-default-user.mjs",
    "assign-devices": "node scripts/assign-all-devices-to-admin.mjs",
    "assign-new-devices": "node scripts/auto-assign-new-devices.mjs",
    "add-all-devices-to-admin": "node scripts/add-all-devices-to-admin.mjs"
  },
  "dependencies": {
